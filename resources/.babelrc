{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "ie": 11,
          "android": 4
        },
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "debug": true
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
